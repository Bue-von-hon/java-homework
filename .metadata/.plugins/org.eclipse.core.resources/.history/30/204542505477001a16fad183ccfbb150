
public class Stuff {
	private String iName;
	private int qun;
	
	public Stuff(String iName, int qun)
	{
		this.iName = iName;
		this.qun = qun;
	}
	
	public String GetIName()
	{
		return iName;
	}
	
	public int GetQun()
	{
		return qun;
	}
	
	public int SetQun(int number)
	{
		if(this.qun < number)
		{
			this.qun = 0;
		}
		else
		
		{
			
		}
	}
}
